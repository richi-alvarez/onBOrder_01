<h1><%= nombrePagina %></h1>
<section class="bgwhite p-t-55 p-b-65">
    <div class="container">
        <div class="row">
         
    <% if (products) { %>
  
        <div class="col-sm-6 col-md-8 col-lg-11 p-b-50">
            <div class="row" id="list">
                <% products.forEach(producto => {  %>
                    <div  class="col-sm-12 col-md-6 col-lg-3 p-b-50">
                        <!-- Block2 -->
                        <div class="block2">
                            <div class="block2-img wrap-pic-w of-hidden pos-relative block2-labelnew">
                                <% if(producto.item.imagen) { %>
                                    <img src="/uploads/productos/<%= producto.item.imagen %>" alt="IMG-PRODUCT" style="height: 327px;">
                                <% } else { %>
                                    <img src="/img/meeti.jpg" alt="IMG-PRODUCT" style="height: 327px;">
                                <% } %>
                                <div class="block2-overlay trans-0-4">
                                    <a href="#" class="block2-btn-addwishlist hov-pointer trans-0-4">
                                        <i class="fa fa-heart icon_heart_alt" aria-hidden="true"></i>
                                        <i class="fa fa-heart icon_heart dis-none" style="color:red" aria-hidden="true"></i>
                                    </a>
                    
                                    <div class="block2-btn-addcart w-size1 trans-0-4"  data-id="/add-to-cart/<%= producto.item.id %>">
                                        <!-- Button -->
                                        <button class="flex-c-m size1 bg4 bo-rad-23 hov1 s-text1 trans-0-4" >
                                            Add to Cart 
                                        </button>
                                    </div>
                                </div>
                            </div>
                    
                            <div class="block2-txt p-t-20">
                                    <a href="/meeti/<%= producto.item.slug %>" class="block2-name dis-block s-text3 p-b-5">
                                    <%= producto.item.titulo %>
                                </a>
                    
                                <span class="block2-price m-text6 p-r-5">
                                    $<%= producto.item.valorMeeti %>
                                </span>
                            </div>
                        </div>
                    </div>

                <% }) %>
    
            </div>
        </div>
    <% } else { %>
        <p>No productos en mi mlista de deseos</p>
    <% } %>
        </div>
    </div>
</section>
