<div class="item-slick2 p-l-15 p-r-15">
    <!-- Block2 -->
    <div class="block2">
        <!-- productos relacionados mostrarMeeti controller -->
        <% if(meetisDos.nuevo == 'nuevo') { %>
            <div class="block2-img wrap-pic-w of-hidden pos-relative block2-labelnew" style="height: 300px;">
        <% } else { %>
                <div class="block2-img wrap-pic-w of-hidden pos-relative" style="height: 300px;">
            <% } %>
     
            <% if(meetisDos.imagen) { %>
            <img src="/uploads/productos/<%= meetisDos.imagen %>" alt="IMG-PRODUCT">
            <% } else { %>
                <img src="/img/meeti.jpg" alt="IMG-PRODUCT" >
            <% } %>

            <div class="block2-overlay trans-0-4"  data-id="/add-to-wish/<%= meeti.id %>">
                <a href="#" class="block2-btn-addwishlist hov-pointer trans-0-4">
                    <i class="fa fa-heart icon_heart_alt" aria-hidden="true"></i>
                    <i class="fa fa-heart icon_heart dis-none" style="color:red" aria-hidden="true"></i>
                </a>

                <div class="block2-btn-addcart w-size1 trans-0-4" data-id="add-to-cart/<%= meetisDos.id %>">
                    <!-- Button -->
                    <button class="flex-c-m size1 bg4 bo-rad-23 hov1 s-text1 trans-0-4" >
                        Add to Cart
                    </button>
                </div>
            </div>
        </div>

        <div class="block2-txt p-t-20">
            <a href="/meeti/<%= meetisDos.slug %>" class="block2-name dis-block s-text3 p-b-5">
                <%= meetisDos.titulo %>
            </a>

            <span class="block2-price m-text6 p-r-5">
                <%= meetisDos.valorMeeti %>
            </span>
        </div>
    </div>
</div>